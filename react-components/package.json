    "scripts": {
        "build": "npm run clean && babel src -d lib",
        "clean": "rimraf lib",
        "deploy": "build-storybook",
        "flow": "flow",
        "git:add": "git add --all",
        "git:commit": "git commit -m \"updated code + documentation\"",
        "git:push": "git push origin master",
        "git:release": "npm -s run git:add && npm -s run git:commit && npm -s run git:push",
        "git:tag": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
        "release": "npm -s run git:release && npm version patch && git push --tags && npm run build && npm publish",
        "storybook": "start-storybook -s ./docs -p 6006",
        "test": "jest"
    },
